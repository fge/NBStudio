<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    
    <fontcolor name="command"       foreColor="blue"/>
    <fontcolor name="function"       foreColor="blue"/>
    <fontcolor name="specvariable"       foreColor="blue"/>
    <fontcolor name="localvariable"    foreColor="ff800000" default="identifier"/>    
    <fontcolor name="number"        default="number"/>    
    <fontcolor name="comment"       foreColor="ff008000"/>
    <fontcolor name="character"     foreColor="ff008000" default="character"/>
    <fontcolor name="keyword"       default="keyword"/>
    <fontcolor name="label"       foreColor="red"/>
</fontscolors>